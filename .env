# This is a template file. For the app to work, create a .env.local file
# and add your actual Firebase and Google AI credentials there.

# --- Firebase Project Credentials ---
# Get these from your Firebase project settings (Config section)
# https://console.firebase.google.com/
NEXT_PUBLIC_FIREBASE_API_KEY="your-api-key"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="your-auth-domain"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="your-project-id"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="your-storage-bucket"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="your-messaging-sender-id"
NEXT_PUBLIC_FIREBASE_APP_ID="your-app-id"
NEXT_AON_MEASUREMENT_ID="your-measurement-id"

# --- Google AI (Gemini) API Key ---
# Required for the AI Motivation feature.
# Get this from Google AI Studio: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY="your-gemini-api-key"
